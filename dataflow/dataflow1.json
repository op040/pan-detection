{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "empdata",
						"type": "DatasetReference"
					},
					"name": "splitonthebasisoftrack"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "PaaS"
				}
			],
			"transformations": [
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Emp as string,",
				"          Name as string,",
				"          Location as string,",
				"          Track as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> splitonthebasisoftrack",
				"splitonthebasisoftrack split(equals(Track,\"PaaS\"),",
				"     equals(Track, \"SaaS\"),",
				"     equals(Track, \"IaaS\"),",
				"     disjoint: false) ~> split1@(PaaS, SaaS, IaaS, oother)",
				"split1@PaaS sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> PaaS"
			]
		}
	}
}